    "@babel/preset-env": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "jest": "^30.0.2",
    "ts-jest": "^29.1.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": [
